primary\" dark class=\"mb-2\" v-on=\"on\">Agregar Cliente</v-btn>\r\n          </template>\r\n          <v-card>\r\n            <v-card-title>\r\n              <span class=\"headline\">{{ formTitle }}</span>\r\n            </v-card-title>\r\n\r\n            <v-card-text>\r\n              <v-container>\r\n                <v-row>\r\n                  <v-col cols=\"12\" sm=\"12\" md=\"12\">\r\n                    <v-text-field type=\"number\" v-model=\"cedula\" label=\"Cedula\"></v-text-field>\r\n                  </v-col>\r\n                  <v-col cols=\"12\" sm=\"12\" md=\"12\">\r\n                    <v-text-field v-model=\"name\" label=\"Nombre\"></v-text-field>\r\n                  </v-col>\r\n                  <v-col cols=\"12\">\r\n                    <v-select\r\n                    v-model=\"categoryName\"\r\n                      :items=\"categories\"\r\n                      :menu-props=\"{ top: true, offsetY: true }\"\r\n                      label=\"Categoria\"\r\n                    ></v-select>\r\n                  </v-col>\r\n                  <v-col cols=\"12\" sm=\"12\" md=\"12\">\r\n                    <v-text-field type=\"number\" v-model=\"telefono\" label=\"Telefono\"></v-text-field>\r\n                  </v-col>\r\n                  <v-col cols=\"12\" sm=\"12\" md=\"12\">\r\n                    <v-text-field v-model=\"email\" label=\"Email\"></v-text-field>\r\n                  </v-col>\r\n                </v-row>\r\n              </v-container>\r\n            </v-card-text>\r\n\r\n            <v-card-actions>\r\n              <v-spacer></v-spacer>\r\n              <v-btn color=\"blue darken-1\" text @click=\"close\">Cancelar</v-btn>\r\n              <v-btn color=\"blue darken-1\" text @click=\"saveClient\">Guardar</v-btn>\r\n            </v-card-actions>\r\n          </v-card>\r\n        </v-dialog>\r\n      </v-toolbar>\r\n    </template>\r\n    <template v-slot:item.actions=\"{ item }\">\r\n      <v-icon small class=\"mr-2\" @click=\"editarClient(item)\">mdi-pencil</v-icon>\r\n      <v-icon small @click=\"deleteClient(item)\">mdi-delete</v-icon>\r\n    </template>\r\n    <template v-slot:no-data>\r\n      <v-btn color=\"primary\" @click=\"getClient\">Reset</v-btn>\r\n    </template>\r\n  </v-data-table>\r\n</template>\r\n<script>\r\nimport axios from \"axios\";\r\nexport default {\r\n  data: () => ({\r\n    Clients: [],\r\n    dialog: false,\r\n    search: \"\",\r\n    loading:false,\r\n    categories: [\"Regurlar\", \"Primiun\"],\r\n    categoryName:'',\r\n    headers: [\r\n      { text: \"Cedula\", value: \"cedula\", sortable: true },\r\n      { text: \"Nombre\", value: \"nombre\" },\r\n      { text: \"Telefono\", value: \"telefono\" },\r\n      { text: \"Email\", value: \"email\" },\r\n      { text: \"Categoria\", value: \"category\" },\r\n\r\n      { text: \"Actions\", value: \"actions\", sortable: false }\r\n    ],\r\n    editedIndex: -1,\r\n    editedItem: {\r\n      name: \"\",\r\n      calories: 0,\r\n      fat: 0,\r\n      carbs: 0,\r\n      protein: 0\r\n    },\r\n\r\n    cedula: \"\",\r\n    name: \"\",\r\n    telefono: \"\",\r\n    email: \"\",\r\n    defaultItem: {\r\n      name: \"\",\r\n      calories: 0,\r\n      fat: 0,\r\n      carbs: 0,\r\n      protein: 0\r\n    }\r\n  }),\r\n  //vigila cuando una propiedad cambia\r\n  computed: {\r\n    formTitle() {\r\n      return this.editedIndex === -1 ? \"Nuevo Cliente\" : \"Editar Cliente\";\r\n    }\r\n  },\r\n\r\n  watch: {\r\n    dialog(val) {\r\n      val || this.close();\r\n    }\r\n  },\r\n\r\n  created() {\r\n    this.getClient();\r\n  },\r\n\r\n  methods: {\r\n    cleanClient() {\r\n      this.name = \"\";\r\n      this.cedula = \"\";\r\n      this.telefono = \"\";\r\n      this.email = \"\";\r\n      this.categoryName = '';\r\n    },\r\n    getClient() {\r\n      this.loading = true\r\n      axios.get(\"Clients/GetClient\").then(response => {\r\n        this.Clients = response.data;\r\n        if(!this.Clients.length){\r\n          this.loading = true;\r\n        }else{\r\n          this.loading = false;\r\n        }\r\n      })\r\n    },\r\n    saveClient() {\r\n      if (this.editedIndex > -1) {\r\n        axios\r\n          .put(\"Clients/EditClient\", {\r\n            id: this.id,\r\n            cedula: this.cedula,\r\n            nombre: this.name,\r\n            telefono: this.telefono,\r\n            email: this.email,\r\n            category:this.categoryName\r\n\r\n          })\r\n          .then(() => {\r\n            this.getClient();\r\n            this.close();\r\n            this.cleanClient();\r\n          });\r\n      } else {\r\n        axios\r\n          .post(\"Clients/CreateClient\", {\r\n            cedula: this.cedula,\r\n            nombre: this.name,\r\n            telefono: this.telefono,\r\n            email: this.email,\r\n            category:this.categoryName\r\n          })\r\n          .then(() => {\r\n            this.getClient();\r\n            this.close();\r\n            this.cleanClient();\r\n          });\r\n      }\r\n    },\r\n\r\n    editarClient(Client) {\r\n      this.id = Client.id;\r\n      this.cedula = Client.cedula;\r\n      this.name = Client.nombre;\r\n      this.telefono = Client.telefono;\r\n      this.email = Client.email;\r\n      this.categoryName = Client.categoryName\r\n      this.editedIndex = 1;\r\n      this.dialog = true;\r\n    },\r\n\r\n    deleteClient(item) {\r\n      this.$confirm(\r\n        `Estas seguro que deseas eliminar este proveedor${item.nombre}`\r\n      ).then(() => {\r\n        axios.put(\"Clients/DeleteClient/\" + item.id).then(() => {\r\n          this.getClient();\r\n        });\r\n      });\r\n    },\r\n\r\n    close() {\r\n      this.dialog = false;\r\n      setTimeout(() => {\r\n        this.editedItem = Object.assign({}, this.defaultItem);\r\n        this.editedIndex = -1;\r\n      }, 300);\r\n    }\r\n  }\r\n};\r\n</script>"],"sourceRoot":"src/components/clientComponent"}]}