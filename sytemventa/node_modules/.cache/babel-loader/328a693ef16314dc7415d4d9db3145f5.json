CA,SAFA;AAGA,OAJA;AAMA,KAxDA;AA0DA,IAAA,KA1DA,mBA0DA;AAAA;;AACA,WAAA,MAAA,GAAA,KAAA;AACA,MAAA,UAAA,CAAA,YAAA;AACA,QAAA,MAAA,CAAA,UAAA,GAAA,MAAA,CAAA,MAAA,CAAA,EAAA,EAAA,MAAA,CAAA,WAAA,CAAA;AACA,QAAA,MAAA,CAAA,WAAA,GAAA,CAAA,CAAA;AACA,OAHA,EAGA,GAHA,CAAA;AAIA;AAhEA;AA9CA,CAAA","sourcesContent":["<template>\r\n  <v-data-table :headers=\"headers\" :search=\"search\" no-results-text=\"No existen registro\" :items=\"products\" sort-by=\"calories\" class=\"elevation-1\">\r\n    <template v-slot:top>\r\n      <v-toolbar flat color=\"white\">\r\n        <v-toolbar-title>Productos</v-toolbar-title>\r\n        <v-divider class=\"mx-4\" inset vertical></v-divider>\r\n                  <v-text-field\r\n        v-model=\"search\"\r\n        \r\n        append-icon=\"mdi-magnify\"\r\n        label=\"Search\"\r\n        single-line\r\n        hide-details\r\n      ></v-text-field>\r\n        <v-spacer></v-spacer>\r\n \r\n        <v-dialog v-model=\"dialog\" max-width=\"500px\">\r\n          <template v-slot:activator=\"{ on }\">\r\n            <v-btn color=\"primary\" dark class=\"mb-2\" v-on=\"on\">Agregar Producto</v-btn>\r\n          </template>\r\n          <v-card>\r\n            <v-card-title>\r\n              <span class=\"headline\">{{ formTitle }}</span>\r\n            </v-card-title>\r\n\r\n            <v-card-text>\r\n              <v-container>\r\n                <v-row>\r\n                  <v-col cols=\"12\" sm=\"12\" md=\"12\">\r\n                    <v-text-field v-model=\"name\" label=\"Nombre\"></v-text-field>\r\n                  </v-col>\r\n                  <v-col cols=\"12\" sm=\"12\" md=\"12\">\r\n                    <v-text-field type=\"number\" v-model=\"price\" label=\"Precio\"></v-text-field>\r\n                  </v-col>\r\n                </v-row>\r\n              </v-container>\r\n            </v-card-text>\r\n\r\n            <v-card-actions>\r\n              <v-spacer></v-spacer>\r\n              <v-btn color=\"blue darken-1\" text @click=\"close\">Cancelar</v-btn>\r\n              <v-btn color=\"blue darken-1\" text @click=\"saveProduct\">Guardar</v-btn>\r\n            </v-card-actions>\r\n          </v-card>\r\n        </v-dialog>\r\n      </v-toolbar>\r\n    </template>\r\n    <template v-slot:item.actions=\"{ item }\">\r\n      <v-icon small class=\"mr-2\" @click=\"editarProduct(item)\">mdi-pencil</v-icon>\r\n      <v-icon small @click=\"deleteProduct(item)\">mdi-delete</v-icon>\r\n    </template>\r\n    <template v-slot:no-data>\r\n      <v-btn color=\"primary\" @click=\"getProduct\">Reset</v-btn>\r\n    </template>\r\n  </v-data-table>\r\n</template>\r\n<script>\r\nimport axios from \"axios\";\r\nexport default {\r\n  data: () => ({\r\n    products: [],\r\n    dialog: false,\r\n    search:'',\r\n    headers: [\r\n      { text: \"Nombre\", value: \"nombre\", sortable: true },\r\n      { text: \"Precio\", value: \"precio\" },\r\n      { text: \"Actions\", value: \"actions\", sortable: false }\r\n    ],\r\n    editedIndex: -1,\r\n    editedItem: {\r\n      name: \"\",\r\n      calories: 0,\r\n      fat: 0,\r\n      carbs: 0,\r\n      protein: 0\r\n    },\r\n\r\n    name: \"\",\r\n    price: \"\",\r\n    defaultItem: {\r\n      name: \"\",\r\n      calories: 0,\r\n      fat: 0,\r\n      carbs: 0,\r\n      protein: 0\r\n    }\r\n  }),\r\n  //vigila cuando una propiedad cambia\r\n  computed: {\r\n    formTitle() {\r\n      return this.editedIndex === -1 ? \"Nuevo Producto\" : \"Editar Producto\";\r\n    }\r\n  },\r\n\r\n  watch: {\r\n    dialog(val) {\r\n      val || this.close();\r\n    }\r\n  },\r\n\r\n  created() {\r\n    this.getProduct();\r\n  },\r\n\r\n  methods: {\r\n    cleanProduct(){\r\n    this.name = '';\r\n    this.price ='';\r\n    \r\n  },\r\n\r\n    getProduct() {\r\n      axios.get(\"Products/GetProduct\").then(response => {\r\n        this.products = response.data;\r\n      });\r\n      console.log(this.product);\r\n    },\r\n    saveProduct() {\r\n      if (this.editedIndex > -1) {\r\n        axios\r\n          .put(\"Products/EditProduct\", {\r\n            id: this.id,\r\n            nombre: this.name,\r\n            precio: this.price\r\n          })\r\n          .then(() => {\r\n            this.getProduct();\r\n            this.close();\r\n            this.cleanProduct();\r\n          });\r\n      } else {\r\n        axios\r\n          .post(\"Products/CreateProduct\", {\r\n            nombre: this.name,\r\n            precio: this.price\r\n          })\r\n          .then(() => {\r\n            this.getProduct();\r\n            this.close();\r\n            this.cleanProduct();\r\n          });\r\n      }\r\n    },\r\n\r\n    editarProduct(product) {\r\n      this.id = product.id;\r\n      this.name = product.nombre;\r\n      this.price = product.precio;\r\n\r\n      this.editedIndex = 1;\r\n      this.dialog = true;\r\n    },\r\n\r\n    deleteProduct(item) {\r\n      this.$confirm(`Estas seguro que deseas eliminar el producto${item.nombre}`).then(() => {\r\n              axios.put('Products/DeleteProduct/'+item.id).then(() =>{\r\n          this.getProduct();\r\n        })\r\n});\r\n        \r\n    },\r\n\r\n    close() {\r\n      this.dialog = false;\r\n      setTimeout(() => {\r\n        this.editedItem = Object.assign({}, this.defaultItem);\r\n        this.editedIndex = -1;\r\n      }, 300);\r\n    }\r\n  }\r\n};\r\n</script>"],"sourceRoot":"src/components/productComponent"}]}